{
  "name": "beacons-backoffice",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src"
  },
  "dependencies": {
    "@appbaseio/reactivesearch": "^3.39.0-alpha.4",
    "@azure/msal-browser": "^2.39.0",
    "@azure/msal-react": "^1.0.0",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@fontsource/roboto": "^4.0.0",
    "@material-table/core": "^0.2.35",
    "@mui/icons-material": "^5.4.2",
    "@mui/material": "^5.4.2",
    "@mui/styles": "^5.4.2",
    "@mui/x-data-grid": "^5.5.1",
    "@mui/x-data-grid-pro": "^5.5.1",
    "@testing-library/react": "^12.1.3",
    "@types/lodash": "^4.14.182",
    "@types/node": "^20.0.0",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "axios": "^1.9.0",
    "css-select": "^5.1.0",
    "date-fns": "^4.1.0",
    "formik": "^2.2.9",
    "inflected": "^2.1.0",
    "lodash": "^4.17.21",
    "polished": "^4.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.39.3",
    "react-router-dom": "^5.2.0",
    "typescript": "^5.9.2",
    "uuid": "^11.1.0",
    "web-vitals": "^2.1.0",
    "yup": "^1.1.1"
  },
  "overrides": {
    "follow-redirects": "^1.14.9",
    "@svgr/webpack": "$@svgr/webpack"
  },
  "engines": {
    "node": "20.19.x",
    "npm": "10.8.x"
  },
  "lint-staged": {
    "*.{js, jsx, ts, tsx}": "eslint --cache --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "/backoffice",
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "clearMocks": true,
    "testEnvironment": "jest-environment-jsdom",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(uuid|@material-table/core)/)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/utils/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/src/utils/__mocks__/styleMock.ts",
      "^uuid$": "uuid"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ]
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.27.0",
    "@stryker-mutator/core": "^9.0.1",
    "@stryker-mutator/jest-runner": "^9.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@vitejs/plugin-react": "^4.4.1",
    "enhanced-resolve": "^5.9.0",
    "eslint": "^9.27.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "express": "^5.1.0",
    "globals": "^16.1.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.0.0",
    "sass": "^1.54.5",
    "ts-jest": "^29.3.4",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.6",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "proxy": "http://localhost:3005"
}
